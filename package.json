{
  "name": "trevordmiller-website",
  "scripts": {
    "dev": "next",
    "format": "prettier --write '{components,pages,utils}/**/*.js'",
    "lint": "eslint . --fix",
    "build": "next build",
    "verify": "npm run lint && npm run build",
    "precommit": "npm run format && npm run verify",
    "stage": "git checkout master && git pull && now",
    "release": "now alias",
    "postrelease": "now -e REDIRECT_URL=https://trevordmiller.com timneutkens/now-redirect && now alias $(pbpaste) www.trevordmiller.com",
    "start": "next start"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "now": "^10.2.2",
    "prettier": "^1.11.1"
  },
  "dependencies": {
    "next": "^5.1.0",
    "nova-colors": "^2.1.5",
    "prop-types": "^15.6.1",
    "raven-js": "^3.24.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-syntax-highlighter": "^7.0.2",
    "react-tabs": "^2.2.1",
    "rss": "^1.2.2"
  }
}
